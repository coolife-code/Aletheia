{
  "sdk_type": "gradio",
  "resource_configuration": {
    "cpu": 2,
    "memory": 4,
    "gpu": 0
  },
  "environment_variables": [
    {
      "name": "OPENAI_API_KEY",
      "value": "",
      "description": "阿里百炼 API密钥 (用于DeepSeek-V3.2模型)"
    },
    {
      "name": "OPENAI_BASE_URL",
      "value": "https://dashscope.aliyuncs.com/compatible-mode/v1",
      "description": "阿里百炼 API基础URL"
    },
    {
      "name": "OPENAI_MODEL",
      "value": "deepseek-v3.2",
      "description": "使用的模型名称"
    },
    {
      "name": "LLM_PROVIDER",
      "value": "openai",
      "description": "LLM提供商 (固定为openai，使用阿里百炼兼容接口)"
    },
    {
      "name": "VERDICT_LLM_MODEL",
      "value": "deepseek-v3.2",
      "description": "Verdict Agent专用模型"
    },
    {
      "name": "VERDICT_LLM_TEMPERATURE",
      "value": "0.1",
      "description": "Verdict Agent温度参数"
    }
  ]
}
